<?xml version="1.0"?>
<Container version="2">
  <Name>grafana</Name>
  <Repository>grafana/grafana</Repository>
  <Registry>https://hub.docker.com/r/grafana/grafana/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/index.php?topic=51498.0</Support>
  <Project>http://grafana.org/</Project>
  <Overview>Grafana is an open source, feature rich metrics dashboard and graph editor for Graphite, Elasticsearch, OpenTSDB, Prometheus and InfluxDB.</Overview>
  <Category>Network:Management Tools:Utilities spotlight:</Category>
  <WebUI>http://[IP]:[PORT:3000]/</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/atribe/unRAID-docker/master/grafana.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/dyf991645/unraid-docker-templates/main/icons/grafana.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1687605126</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Host Port 1" Target="3000" Default="3000" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false">3000</Config>
  <Config Name="Host Path 1" Target="/var/lib/grafana" Default="/mnt/user/appdata/grafana" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/grafana</Config>
  <Config Name="Key 1" Target="GF_SERVER_ROOT_URL" Default="http://your.server.ip.or.hostname" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">http://192.168.31.101</Config>
  <Config Name="Key 2" Target="GF_SECURITY_ADMIN_PASSWORD" Default="admin" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">admin</Config>
</Container>
