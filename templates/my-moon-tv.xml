  <Config Name="web" Target="3000" Default="3000" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">3002</Config>
  <Config Name="USERNAME" Target="USERNAME" Default="admin" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">admin</Config>
  <Config Name="PASSWORD" Target="PASSWORD" Default="admin_password" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="true">******</Config>
