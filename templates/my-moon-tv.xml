<?xml version="1.0"?>
<Container version="2">
  <Name>moon-tv</Name>
  <Repository>ghcr.io/senshinya/moontv:latest</Repository>
  <Registry>https://github.com/senshinya/MoonTV</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project/>
  <Overview/>
  <Category/>
  <WebUI>http://[IP]:[PORT:8070]</WebUI>
  <TemplateURL/>
