  <Config Name="REDIS_URL" Target="REDIS_URL" Default="redis://172.17.0.1:6379" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">redis://172.17.0.1:6379</Config>
  <Config Name="NEXT_PUBLIC_ENABLE_REGISTER" Target="NEXT_PUBLIC_ENABLE_REGISTER" Default="true" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">true</Config>
